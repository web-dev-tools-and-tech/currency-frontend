{
  "extends": "react-app",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
        "error",
        {
            "semi": false,
            "parser": "babylon",
            "printWidth": 100,
            "singleQuote": true,
            "trailingComma": "all",
            "bracketSpacing": false,
            "jsxBracketSameLine": true
        }
    ]
  }
}